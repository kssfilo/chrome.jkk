{
  "manifest_version": 2,
  "name": "Simple",
  "version": "0.0",
  "description": "Simple Extension",
  "background": {
  	"scripts": ["background.js"],
	"persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
	"default_popup": "popup.html",
	"default_title": "Simple Extension"
  },
  "content_scripts": [
  	{
		"matches": ["*://*/*"],
		"js": ["contentscript.js"],
		"run_at": "document_end"
	}
  ]
}
